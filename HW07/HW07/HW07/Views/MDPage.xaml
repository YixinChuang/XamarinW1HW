<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
                  prism:ViewModelLocator.AutowireViewModel="True"
                  x:Class="HW07.Views.MDPage">
  <MasterDetailPage.Master>
    <ContentPage Title="主功能表">
      <StackLayout HorizontalOptions="Center" VerticalOptions="Center" Orientation="Vertical">
        <Button Text="Page1" Command="{Binding Page1Command}" />
        <Button Text="Page2" Command="{Binding Page2Command}" />
      </StackLayout>
    </ContentPage>
  </MasterDetailPage.Master>
</MasterDetailPage>